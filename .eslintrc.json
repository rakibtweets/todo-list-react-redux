{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": "off",
    "jsx-a11y/control-has-associated-label": 0,
    "import/prefer-default-export": 0,
    "arrow-body-style": ["off", "always"],
    "indent": ["warn", 2],
    "comma-dangle": "off"
  }
}
